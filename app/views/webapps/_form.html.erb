<%= semantic_form_for @webapp do |f| %>
  <%= f.inputs do %>
    <%= f.input :project_name %>
    <%= f.input :language %>
    <%= f.input :framework %>
    <%= f.input :description %>
    <%= f.input :notes %>
  <% end %>
  <%= f.actions do %>
    <%= f.action :submit, :as => :button %>
  <% end %>
<% end %>
